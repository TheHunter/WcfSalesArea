    <tags>NHibernate DAO DAL ORM</tags>
    <dependencies>
      <group targetFramework=".NETFramework3.5">
        <dependency id="NHibernate" version="3.3.3.4001" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.ServiceModel" targetFramework=".NETFramework3.5" />
      <frameworkAssembly assemblyName="System.ServiceModel.Web" targetFramework=".NETFramework3.5" />
    </frameworkAssemblies>
  </metadata>
</package>